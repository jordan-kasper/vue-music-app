n          <th scope=\"col\">Artist</th>\n          <th scope=\"col\">Play</th>\n          <th scope=\"col\">Stop</th>\n        </tr>\n      </thead>\n      <tbody v-for=\"song in playList\" v-bind:key=\"song.id\">\n        <tr class=\"text-left\">\n          <td @click=\"deleteSong(song)\"><b-icon icon=\"x-circle\" scale=\"2\" variant=\"danger\">\n            </b-icon></td>\n          <td>{{ song.title }}</td>\n          <td>{{ song.artist.name }}</td>\n          <td @click=\"playAudio(song.preview)\"><b-icon icon=\"play\" style=\"width: 30px;\n          height: 30px;\" class=\"rounded-circle bg-info\" variant=\"light\"></b-icon></td>\n          <td @click=\"stopAudio()\"><b-icon icon=\"stop\" style=\"width: 30px; height: 30px;\"\n          class=\"rounded-circle bg-info\" variant=\"light\"></b-icon></td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'playlist',\n  props: ['playList'],\n  methods: {\n    deleteSong(song) {\n      const songIndex = this.playList.indexOf(song);\n      this.playList.splice(songIndex, 1);\n    },\n    playAudio(url) {\n      this.playingSong = new Audio(url);\n      this.playingSong.play();\n    },\n    stopAudio() {\n      this.playingSong.pause();\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components"}]}