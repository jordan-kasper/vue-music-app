AA;AACA,IAAA,KAAA,EAAA;AADA,GA7BA;AAgCA,EAAA,QAAA,EAAA;AACA,IAAA,cADA,4BACA;AAAA;;AACA;AACA,aAAA,KAAA,QAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,YAAA,QAAA,GAAA,CAAA;AACA,YAAA,KAAA,CAAA,cAAA,KAAA,MAAA,EAAA,QAAA,GAAA,CAAA,CAAA;AACA,YAAA,CAAA,CAAA,KAAA,CAAA,WAAA,CAAA,GAAA,CAAA,CAAA,KAAA,CAAA,WAAA,CAAA,EAAA,OAAA,CAAA,CAAA,GAAA,QAAA;AACA,YAAA,CAAA,CAAA,KAAA,CAAA,WAAA,CAAA,GAAA,CAAA,CAAA,KAAA,CAAA,WAAA,CAAA,EAAA,OAAA,IAAA,QAAA;AACA,eAAA,CAAA;AACA,OANA,CAAA;AAOA;AAVA;AAhCA,CAAA","sourcesContent":["<template>\n  <div class=\"col-md-4\">\n    <h4 class=\"d-flex justify-content-between align-items-center\">\n      <span>Playlist</span>\n    </h4>\n    <table class=\"table\">\n      <thead>\n        <tr class=\"text-left\">\n          <th scope=\"col\"></th>\n          <th scope=\"col\" @click=\"sort('name')\">Song Name</th>\n          <th scope=\"col\" @click=\"sort('artist')\">Artist</th>\n          <th scope=\"col\">Play</th>\n          <th scope=\"col\">Stop</th>\n        </tr>\n      </thead>\n      <tbody v-for=\"song in SortedplayList\" v-bind:key=\"song.id\">\n        <tr class=\"text-left\">\n          <td name=\"delete\" @click=\"deleteSong(song)\">\n            <b-icon icon=\"x-circle\" scale=\"2\" variant=\"danger\"> </b-icon>\n          </td>\n          <td>{{ song.title }}</td>\n          <td>{{ song.artist.name }}</td>\n          <td @click=\"playAudio(song.preview)\">\n            <button type=\"button\" class=\"btn btn-outline-secondary\">\n              <b-icon\n                icon=\"play-fill\"\n                style=\"width: 30px;\n          height: 30px;\"\n                variant=\"info\"\n              ></b-icon>\n            </button>\n          </td>\n          <td @click=\"stopAudio()\">\n            <button type=\"button\" class=\"btn btn-outline-secondary\">\n              <b-icon\n                icon=\"stop-fill\"\n                style=\"width: 30px; height: 30px;\"\n                variant=\"info\"\n              ></b-icon>\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n      debug: sort={{currentSort}}, dir={{currentSortDir}}\n      playList : {{ playList }}\n\n  </div>\n</template>\n\n<script>\nimport { BIcon } from 'bootstrap-vue';\n\nexport default {\n  name: 'playlist',\n  props: ['playList'],\n  data() {\n    return {\n      currentSort: 'name',\n      currentSortDir: 'asc',\n    };\n  },\n  methods: {\n    deleteSong(song) {\n      const songIndex = this.playList.indexOf(song);\n      this.playList.splice(songIndex, 1);\n    },\n    playAudio(url) {\n      this.playingSong = new Audio(url);\n      this.playingSong.play();\n    },\n    stopAudio() {\n      this.playingSong.pause();\n    },\n    sort(s) {\n      // if s == current sort, reverse\n      if (s === this.currentSort) {\n        this.currentSortDir = this.currentSortDir === 'asc' ? 'desc' : 'asc';\n      }\n      this.currentSort = s;\n    },\n  },\n  components: {\n    BIcon,\n  },\n  computed: {\n    SortedplayList() {\n      // eslint-disable-next-line vue/no-side-effects-in-computed-properties\n      return this.playList.sort((a, b) => {\n        let modifier = 1;\n        if (this.currentSortDir === 'desc') modifier = -1;\n        if (a[this.currentSort] < b[this.currentSort]) return -1 * modifier;\n        if (a[this.currentSort] > b[this.currentSort]) return 1 * modifier;\n        return 0;\n      });\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components"}]}