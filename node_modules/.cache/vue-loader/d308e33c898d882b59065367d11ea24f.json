n          <td>{{ song.title }}</td>\n          <td>{{ song.artist.name }}</td>\n          <td @click=\"playAudio(song.preview)\">\n            <button type=\"button\" class=\"btn btn-outline-secondary\">\n              <b-icon\n                icon=\"play-fill\"\n                style=\"width: 30px;\n          height: 30px;\"\n                variant=\"info\"\n              ></b-icon>\n            </button>\n          </td>\n          <td @click=\"stopAudio()\">\n            <button type=\"button\" class=\"btn btn-outline-secondary\">\n              <b-icon\n                icon=\"stop-fill\"\n                style=\"width: 30px; height: 30px;\"\n                variant=\"info\"\n              ></b-icon>\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n\n  </div>\n</template>\n\n<script>\nimport { BIcon } from 'bootstrap-vue';\n\nexport default {\n  name: 'playlist',\n  props: ['playList'],\n  methods: {\n    deleteSong(song) {\n      const songIndex = this.playList.indexOf(song);\n      this.playList.splice(songIndex, 1);\n    },\n    playAudio(url) {\n      if (this.playingSong === undefined) {\n        this.playingSong = new Audio(url);\n        this.playingSong.play();\n      } else {\n        this.stopAudio();\n        this.playingSong = new Audio(url);\n        this.playingSong.play();\n      }\n    },\n    stopAudio() {\n      this.playingSong.pause();\n    },\n  },\n  components: {\n    BIcon,\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}